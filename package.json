  "main": "index.js",
  "scripts": {
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive -R progress",
    "parallel": "./node_modules/.bin/standard && ./bin/test-parallel",
    "test-node": "./node_modules/.bin/standard && ./node_modules/.bin/mocha --recursive -R progress",
    "test-browser": "./node_modules/.bin/gulp test-karma",
    "test": "npm run test-node && npm run test-browser",
    "build": "./node_modules/.bin/gulp build",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "browserify": {
